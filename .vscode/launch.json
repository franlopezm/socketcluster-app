{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Server Dev Watch",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "nodemon",
      "program": "${workspaceFolder}/server/index.js",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "outputCapture": "std"
    }
  ]
}
